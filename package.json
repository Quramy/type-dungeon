{
  "private": true,
  "name": "type-dungeon",
  "scripts": {
    "clean": "rimraf dist",
    "convert": "ts-node scripts/convert-questions.ts",
    "postconvert": "prettier --write \"dist/**/*.ts\"",
    "markdown": "ts-node -T scripts/to-markdown.ts",
    "postmarkdown": "prettier --write \"dist/*.md\"",
    "build": "yarn convert && yarn markdown",
    "updateReadme": "yarn build && cp dist/README.md ./"
  },
  "keywords": [
    "typescript"
  ],
  "author": "yosuke.kurami@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.18",
    "@types/glob": "^7.1.1",
    "@types/lz-string": "^1.3.33",
    "@types/node": "^13.9.0",
    "glob": "^7.1.6",
    "lz-string": "^1.4.4",
    "markdown-toc": "^1.2.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
