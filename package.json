{
  "private": true,
  "name": "type-dungeon",
  "scripts": {
    "clean": "rimraf dist",
    "prettier": "prettier \"*.json\" \"*.yml\" \"scripts/**/*\" \"questions/**/*\"",
    "format": "yarn prettier --write",
    "format:check": "yarn prettier --check",
    "lint": "yarn format:check",
    "convert": "ts-node scripts/convert-questions.ts",
    "postconvert": "prettier --write \"dist/**/*.ts\"",
    "markdown": "ts-node -T scripts/to-markdown.ts",
    "postmarkdown": "prettier --write \"dist/*.md\"",
    "build": "yarn convert && yarn markdown"
  },
  "keywords": [
    "typescript"
  ],
  "author": "yosuke.kurami@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/tsdoc": "0.12.21",
    "@types/glob": "7.1.3",
    "@types/lz-string": "1.3.34",
    "@types/node": "14.14.8",
    "glob": "7.1.6",
    "husky": "4.3.0",
    "lz-string": "1.4.4",
    "markdown-toc": "1.2.0",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
